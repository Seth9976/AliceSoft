0x00AA68D6:		MSG 0x2f5d1 ; "　"
0x00AA68DC:		CALLFUNC R
0x00AA68E2:		MSG 0x2f5d2 ; "　　魔王リトルプリンセスのその後"
0x00AA68E8:		CALLFUNC R
0x00AA68EE:		MSG 0x2f5d3 ; "　　　　長い間異世界を冒険した美樹と健太郎の二人は皆に見送られて故郷へ帰還する。"
0x00AA68F4:		CALLFUNC A
0x00AA68FA:		MSG 0x2f5d4 ; "ＲＡ１年"
0x00AA6900:		CALLFUNC R
