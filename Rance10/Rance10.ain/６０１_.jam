0x00AA7422:		MSG 0x2f6c2 ; "　"
0x00AA7428:		CALLFUNC R
0x00AA742E:		MSG 0x2f6c3 ; "　　アームズが魔人討伐に成功"
0x00AA7434:		CALLFUNC R
